{
    "builds": [
      {
        "src": "BackEnd/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "/media/$1"
      },
      {
        "src": "/admin/(.*)",
        "dest": "BackEnd/wsgi.py"
      },
      {
        "src": "/api/(.*)",
        "dest": "BackEnd/wsgi.py"
      },
      {
        "src": "/(.*)",
        "dest": "BackEnd/wsgi.py"
      }
    ]
  }